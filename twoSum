nums = [2,7,11,15]

function getIndices(array,target) {

  var resultArray=[]

  for (var i = 0; i<array.length-1; i++) {
    for (var j = i+1; j<array.length; j++) {
      if (array[i] + array[j] === target){
        resultArray.push(i,j)

        return resultArray

      }
    }
  }




}


getIndices(nums,9)
